       <div ng-hide="!proCtrl.gradeDropdownShow" class="filter-dropdown"> 
        <ul class="ul-grade-level"> 
         <li ng-click="proCtrl.clearGrade();">Semua</li> 
         <li ng-click="proCtrl.getNewGradeValue(['K-12']);">K-12</li> 
         <li ng-click="proCtrl.getNewGradeValue(['University']);">Universitas</li> 
         <li ng-click="proCtrl.getNewGradeValue(['University', 'K-12']);">Untuk keduanya</li> 
        </ul> 
       </div> 
