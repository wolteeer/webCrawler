    var requestedVariant = decodeURIComponent((new RegExp('[?|&]' + 'variant' + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search) || [null, ''])[1].replace(/\+/g, '%20')) || null;
    if (requestedVariant) {
      var variantNumber = Number(requestedVariant);
      if (!isNaN(variantNumber)) {
        cxApi.setChosenVariation(variantNumber);
      }
    }

    var isiOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
    var isAndroid = navigator.userAgent.toLowerCase().indexOf("android") > -1;
    if (isiOS || isAndroid) {
      cxApi.setChosenVariation(cxApi.NOT_PARTICIPATING);
    }

    window.chosenVariant = cxApi.chooseVariation();
  </script> 
