     <button class="green-btn how-is-this-different" ng-click="ctrl.howIsThisDifferent();" data-g-click="true" data-g-event="Edu: Canvas" data-g-action="How Is This Different" data-g-label="Core Services">HOW IS THIS DIFFERENT FROM YOUR PERSONAL ACCOUNT?</button> 
     <!-- "directive" --> 
