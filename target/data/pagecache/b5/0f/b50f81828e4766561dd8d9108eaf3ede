         <li ng-click="proCtrl.getNewRoleValue(['Students']);">For students</li> 
         <li ng-click="proCtrl.getNewRoleValue(['Educators']);">For educators</li> 
         <li ng-click="proCtrl.getNewRoleValue(['Students', 'Educators']);">For both</li> 
        </ul> 
       </div> 
      </div> 
