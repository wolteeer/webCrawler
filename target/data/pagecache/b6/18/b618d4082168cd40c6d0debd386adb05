       <span class="plusone-button" id="plusone-httpwwwgooglecoma"></span> 
       <script>
              window.___gcfg = {
      lang: 'ru',
      parsetags: 'explicit'
    };

    var gweb = gweb || {};  // namespace
    gweb.plusOneButton = {};
    gweb.plusOneButton.callbacks = {};
    gweb.plusOneButton.onPlusOneLoad = function() {
      for (var slug in gweb.plusOneButton.callbacks) {
        gapi.plusone.render(slug, gweb.plusOneButton.callbacks[slug]);
      }
      gweb.plusOneButton.callbacks = {};
    };
              </script> 
       <script src="//apis.google.com/js/platform.js"></script> 
       <script>
              gweb.plusOneButton.callbacks['plusone-httpwwwgooglecoma'] = {
      'annotation': 'none',
      'href': 'http://www.google.com/a/',
      'size': 'standard',
      'callback': '',
      'recommendations': 'true'
  };
  gweb.plusOneButton.onPlusOneLoad();
              </script> 
