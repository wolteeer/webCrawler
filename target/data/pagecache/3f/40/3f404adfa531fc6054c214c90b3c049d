  <script>
var dtSrc = 2507573;
    var dtType = 'ads-a681';
    var dtEventConfig = {
      'dt-analytics': 'ads-a694',
      'dt-apps' : 'ads-a161',
      'dt-adsense': 'ads-a866',
      'dt-adwords': 'tqtkq613',
      'dt-awexpress': 'kgdiw043',
      'dt-offers': 'ads-a263',
      'dt-plus': 'ads-a474'
    };

    //DoubleTrack Page Load
    gweb.analytics.DoubleTrack.track('floodlight', {
        src: dtSrc,
        type: dtType,
        cat: 'ads-a460'
    });

    //DoubleTrack Events
    for (var i in dtEventConfig) {
      var tracker = new gweb.analytics.DoubleTrack('floodlight', {
        src: dtSrc,
        type: dtType,
        cat: dtEventConfig[i]
      });

      tracker.trackClass(i, true);
    }
    </script>  
