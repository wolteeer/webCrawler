         <li ng-click="proCtrl.getNewRoleValue(['Students']);">For students</li> 
         <li ng-click="proCtrl.getNewRoleValue(['Educators']);">Educators</li> 
         <li ng-click="proCtrl.getNewRoleValue(['Students', 'Educators']);">For both</li> 
        </ul> 
       </div> 
      </div> 
      <div class="filter-container"> 
       <div class="view-customers filter-select grade-level"> 
        <div class="filter-text-container" ng-click="proCtrl.gradeClick();"> 
         <span class="filter-text">{[proCtrl.search.grade_level.join(', ') || filters[1].translated ]}</span> 
         <div class="filter-arrow" ng-class="{flip: proCtrl.gradeDropdownShow}"></div> 
        </div> 
       </div> 
